{

    "manifest_version": 2,
    "name": "CHAT BOT (client)",
    "version": "0.3",
   
    "description": "Control multimedia para windows a traves del chat de Google Meet",
  
    "icons": {
      "48": "icons/logo-48.png"
    },
    "permissions": ["notifications"],
    "content_scripts": [
      {
        "matches": ["*://meet.google.com/*"],
        "js": ["popup.js","chat_client.js"],
        "css": ["popup/popup.css"]
      }
    ]
  
  }